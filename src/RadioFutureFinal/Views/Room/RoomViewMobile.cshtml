            <div id="div_users_overall" class="info">
                <div class="dropper arrow-down img_drop_right"></div>
                <p id="p_users_summary"></p>
                <div id="div_user_results" class="results"></div>
            </div>
            <div id="div_queue_overall" class="info">
                <p id="p_queue_summary"></p>
                <div class="dropper arrow-down img_drop_left"></div>
                <div id="div_queue_results" class="results"></div>
            </div>
        </div>

        <div id="div_search">
            <input class="myinput" id="input_name" placeholder="type in your name and press enter">
            <input class="myinput" id="input_search" placeholder="search for podcasts">
        </div>

        <div id="div_chat_results">
            <ul id="ul_chat"></ul>
            <input class="myinput" id="input_chat" placeholder="Enter your chat message" />
        </div>

        <div id="div_search_results" class="results"></div>

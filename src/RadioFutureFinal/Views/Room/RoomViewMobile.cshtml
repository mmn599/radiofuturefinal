        <div id="div_sel">
            <span id="btn_sel_queue" class="span_sel">queue</span>
            <span id="btn_sel_search" class="span_sel">search</span>
            <span id="btn_sel_users" class="span_sel">users</span>
        </div>

        <div id="div_stuff">
            <div id="div_queue_results" class="results stuff"></div>
            <div id="div_search" class="stuff">
                <div id="div_search_in">
                    <input autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false" class="myinput" id="input_search" placeholder="search for podcasts">
                    <img id="btn_search" src="~/images/search.jpg" />
                </div>
                <div id="div_search_results" class="results"></div>
            </div>
            <div id="div_user_results" class="results stuff">
                <input autocomplete="off" autocorrect="off" autocapitalize="on" spellcheck="false" class="myinput" id="input_name" placeholder="type in your name and press enter">
                <div id="div_inner_user_results"></div>
            </div>
        </div>

        <div id="div_current_content">
            <canvas id="canvas_progress"></canvas>
            <div class="div_player" id="div_podcast_player">
                <div class="div_inner_podcast_player">
                    <div id="btn_play_pause" class="play_btn"></div>
                    <span id="cc_title"></span>
                    <span id="cc_time" class="time"></span>
                </div>
                <audio id="html5audio"></audio>
            </div>
        </div>


    </div>
</body>

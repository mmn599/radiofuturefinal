<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["roomName"]</title>
    <link href="https://fonts.googleapis.com/css?family=Josefin+Sans&amp;subset=latin,latin-ext" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="~/css/musicroommobile.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Cousine" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Source+Code+Pro:300" rel="stylesheet" type="text/css">
    <script src="~/js/spin.js"></script>
    <script>var mobileBrowser = true;</script>
    <script src="~/js/roombundle.js"></script>
    <script src="https://apis.google.com/js/client.js?onload=ytApiReady"></script>
    <script src="https://www.youtube.com/iframe_api"></script>
</head>
<body>
    <div class="everything" id="div_loading"></div>
    <div class="everything" id="div_everything">

        <div id="div_current_content">
            <div id="div_yt_player"></div>
        </div>

        <div>
            <img id="btn_previous" src="../images/arrows-1.png" class="btn_controls" style="margin-right: 5vw;" />
            <p id="p_cc_summary"> Queue a video! </p>
            <img id="btn_next" src="../images/arrows.png" class="btn_controls" style="margin-left: 5vw;" />
        </div>

        <div style="margin-left: 3vw; margin-right: 3vw; margin-top: 5%; width: 100%; justify-content: center;">
            <input class="myinput" id="input_name" placeholder="enter your name">
            <input class="myinput" id="input_search" placeholder="search for content">
            <div id="div_search_results" class="results"></div>
        </div>

        <p style="margin-top: 8%; margin-left: 2vw; margin-bottom: 1vw;" id="p_users_summary"></p>
        <div id="div_user_results" style="overflow-x: auto; vertical-align: middle; line-height:20vw; white-space: nowrap; display: inline-block; height: 20vw; width: 100%;">
        </div>

        <p style="margin-top: 8%; margin-left: 2vw; margin-bottom: 1vw;" id="p_queue_summary">Nothing in the playlist. Queue something!</p>
        <div id="div_queue_results" style="overflow-x: auto; vertical-align: middle; line-height:20vw; white-space: nowrap; display: inline-block; height: 20vw; width: 100%;">
        </div>

    </div>
</body>
</html>
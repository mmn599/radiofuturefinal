<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["roomName"]</title>
    <link rel="stylesheet" href="~/css/musicroom.css">
    <link href="https://fonts.googleapis.com/css?family=Josefin+Sans&amp;subset=latin,latin-ext" rel="stylesheet" type="text/css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/drawer/3.2.0/css/drawer.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/iScroll/5.1.3/iscroll.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/drawer/3.2.0/js/drawer.min.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Cousine" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Source+Code+Pro:300" rel="stylesheet" type="text/css">
    <script src="~/js/spin.js"></script>
    <script src="~/js/musicroom.js"></script>
    <script src="https://apis.google.com/js/client.js?onload=youtubeAPIInit"></script>
    <script src="https://www.youtube.com/iframe_api?onload=onYouTubeIframeApiReady"></script>
</head>
<body class="drawer drawer--left">
    <div class="everything" id="div_loading"></div>
    <div class="everything" id="div_everything">
        <div id="div_all_info">
            <div id="div_users_overall" style="text-align: center; width: 25%;" class="info">
                <p id="p_users_summary">12 users in the room</p>
                <div id="div_user_results" class="results">
                    <div style="text-align: left; display: flex; align-items: center;">
                        <div style="display: inline-block; margin-right: 16px; height: 48px; width: 48px; background: green;"></div>
                        <span style="margin-right: 16px;">Matthew Normyle<br />Chris Sharma Rock Climbing</span>
                        <img src="~/images/sync.png"/>
                    </div>
                </div>
            </div>
            <div class="info" style="width: 50%;">
                <input id="input_search" placeholder="type in an artist and song name and press enter">
                <div id="div_search_results" class="results"></div>
            </div>
            <div style="width: 25%;" id="div_queue_overall" class="info">
                <p id="p_queue_summary">9 things in the queue</p>
                <div id="div_queue_results" class="results">
                    <p>Queue 1</p>
                    <p>Queue 1</p>
                    <p>Queue 1</p>
                    <p>Queue 1</p>
                    <p>Queue 1</p>
                </div>
            </div>
        </div>
        <div id="div_current_content">
            <div id="div_player"></div>
        </div>
        <div id="div_current_content_info">
            <div id="div_cc_overall" style="width: 33.33%;" class="info">
                <div id="div_cc_results" class="results">
                    <p>Poop 1</p>
                    <p>Poop 1</p>
                    <p>Poop 1</p>
                    <p>Poop 1</p>
                    <p>Poop 1</p>
                </div>
                <p id="p_cc_summary">Rock Climbing 11 Year Old</p>
            </div>
            <div style="width: 33.33%;" class="info">
                <img src="../images/arrows-1.png" style="cursor: pointer" onclick="previousVideoInQueue()"/>
                <img src="../images/pause-button.png" style="margin-left: 5%; margin-right: 5%; cursor: pointer" />
                <img src="../images/arrows.png" style="cursor: pointer" onclick="nextVideoInQueue()"/>
            </div>
            <div id="div_chat_overall" style="width: 33.33%;" class="info">
                <div id="div_chat_results" class="results">
                    <p>Poop 1</p>
                    <p>Poop 1</p>
                    <p>Poop 1</p>
                    <p>Poop 1</p>
                    <p>Poop 1</p>
                </div>
                <p id="p_chat_summary">Chat!</p>
            </div>
        </div>
    </div>
</body>
</html>